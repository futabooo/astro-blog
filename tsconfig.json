{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*.astro"],
      "@layouts/*": ["src/layouts/*.astro"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
